{
  "name": "lesson-next-trpc-firebase",
  "version": "1.0.0",
  "scripts": {
    "prisma-studio": "prisma studio",
    "db-reset": "prisma migrate-dev reset",
    "dx:next": "run-p migrate-dev dev:path dev:next",
    "dx:prisma-studio": "yarn prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "yarn dx:next",
    "dev:next": "next dev",
    "dev:path": "yarn generate:path --watch",
    "build": "next build",
    "lint": "run-s lint:* -c",
    "lint:eslint": "eslint --ext .js,.ts,.jsx,.tsx --max-warnings 0 .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --pretty --noEmit",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "generate:path": "pathpida --enableStatic --ignorePath .gitignore --output src/client/lib/pathpida"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^6.0.2",
    "@mantine/dates": "^6.0.2",
    "@mantine/form": "^6.0.2",
    "@mantine/hooks": "^6.0.2",
    "@mantine/modals": "^6.0.2",
    "@mantine/next": "^6.0.2",
    "@mantine/notifications": "^6.0.2",
    "@prisma/client": "^4.11.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.16.0",
    "@trpc/next": "^10.16.0",
    "@trpc/react-query": "^10.16.0",
    "@trpc/server": "^10.16.0",
    "dayjs": "^1.11.7",
    "firebase": "^9.18.0",
    "next": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-access": "^1.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "prettier": "^2.7.1",
    "prisma": "^4.10.1",
    "typescript": "^4.8.3"
  }
}
